package Code_Abbey;

//  Double Dice Roll


public class Main5 {
    public static void main(String[] args) throws Exception {
        int[] mas1 = {647639463, 2075113061, 30323046, 375133440, 1081625014, 1156786208, 127966739, 517813933, 1043532231, 1802133182, 185930920, 2098100005, 353596625, 695888655, 182179911, 2124810589, 317368838, 2005728588, 485941573, 583483988, 477742947};
        int[] mas2 = {80977356, 1943736724, 1420195914, 1556107952, 2135487357, 1704493589, 2024012067, 2040994978, 1118584809, 1368953221, 106657959, 644312743, 245185634, 2142704252, 1199042382, 1771954042, 1090494256, 312633119, 1661038608, 375237224, 1251210517};
        for (int i = 0; i < mas1.length; i++) {
            double tmp1 = mas1[i] % 6;
            double tmp2 = mas2[i] % 6;
            int res1 = (int) tmp1 + 1;
            int res2 = (int) tmp2 + 1;
            System.out.print(res1 + res2 + " ");
        }
    }
}
